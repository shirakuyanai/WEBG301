<!DOCTYPE html>

<title>OrderDetails</title>

{% block body %}
    <h1>OrderDetails</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ order_detail.id }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ order_detail.total }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ order_detail.status }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ order_detail.createdAt ? order_detail.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ order_detail.updatedAt ? order_detail.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_order_details_index') }}">back to list</a>

    <a href="{{ path('app_order_details_edit', {'id': order_detail.id}) }}">edit</a>

    {{ include('order_details/_delete_form.html.twig') }}
{% endblock %}
