<!DOCTYPE html>

<title>OrderItems</title>

{% block body %}
    <h1>OrderItems</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ order_item.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ order_item.quantity }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ order_item.createdAt ? order_item.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ order_item.updatedAt ? order_item.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_order_items_index') }}">back to list</a>

    <a href="{{ path('app_order_items_edit', {'id': order_item.id}) }}">edit</a>

    {{ include('order_items/_delete_form.html.twig') }}
{% endblock %}
